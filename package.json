{
    "name": "AR-Programming-Language",
    "displayName": "AR Programming Language",
    "description": "ADTECH AR Programming Language extension. Should be used with the AR Programming Language Theme",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "url": "https://github.com/bedesv/AR-Language-Theme",
        "type": "git"
    },
    "author": {
        "name": "Bede Skinner-Vennell",
        "email": "bedesv@gmail.com"
    },
    "contributes": {
        "languages": [{
            "id": "ar",
            "aliases": ["AR Programming Language", "ar"],
            "extensions": [".AR",".ar"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ar",
            "scopeName": "source.AR_Programming_Language",
            "path": "./syntaxes/ar.tmLanguage"
        }]
    }
}